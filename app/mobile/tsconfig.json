{
  "extends": "@react-native/typescript-config",
  "compilerOptions": {
    "types": [
      "jest"
    ],
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@/data/*": [
        "./src/data/*"
      ],
      "@/data": [
        "./src/data"
      ],
      "@/services/*": [
        "./src/services/*"
      ],
      "@/contexts/*": [
        "./src/contexts/*"
      ],
      "@/components/*": [
        "./src/components/*"
      ],
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "**/node_modules",
    "**/Pods"
  ]
}